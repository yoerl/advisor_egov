            <configuration>
              <properties>
                <property>
                  <name>listener</name>
                  <value>org.sonar.java.jacoco.JUnitListener</value>
                </property>
              </properties>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>


    <!--
    The following profiles optimize the build for each team, according to their
     stack layers (batch, server and web teams)
    -->
